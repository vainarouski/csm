{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {

     "alternatives": {
       "type": "array",
       "items": {
         "type": "object",
         "properties": {

           "title": {
             "type": "string"
           },

           "id": {
             "type": "number"
           },

           "url": {
             "url": "string",
             "format": "uri"
           }
         },

         "required": [ "title", "id", "url" ],
         "additionalProperties": false
       },
       "minItems": 1,
       "maxItems": 10,
       "uniqueItems": true
     },

     "anyGood": { "type": "string" },

     "comingSoon": { "type": "boolean" },

     "contentGrid": {
       "patternProperties": {
        "^[a-z]+$": {
          "rating": {
            "type": "number",
            "minimum": 0,
            "maximum": 5
          },
          "text": { "type": "string" },
          "label": { "type": "string" }
        },
        "required": [ "rating", "text", "label" ],
        "additionalProperties": false
       }
     },

     "description": {"type": "string"},

     "oneLiner": {"type": "string"},

     "parentsNeedToKnow": {"type": "string"},

     "product": {
       "type": "object",
       "properties": {

         "cast": {
           "type": "object",
           "patternProperties": {
             "^[0-9]+$": {
               "type": "string",
               "minLength": 2
             }
           },
           "maxProperties": 5
         },

         "directors": {
           "type": "object",
           "patternProperties": {
             "^[0-9]+$": {
               "type": "string",
               "minLength": 2
             }
           },
           "maxProperties": 5
         },

         "genres": {
           "type": "object",
           "patternProperties": {
             "^[0-9]+$": {
               "type": "string",
               "minLength": 2
             }
           },
           "maxProperties": 5
         },

         "length": {
           "properties": {
             "units": {
               "enum": ["minutes"]
             },
             "value": {
               "type": "string",
               "pattern": "^[0-9]+$"
             }
           },
           "required": [
             "units",
             "value"
           ],
           "additionalProperties": false
         },

         "studios": {
           "patternProperties": {
             "^[0-9]+$": {
               "type": "string",
               "minLength": 2
             },
             "minProperties": 1
           }
         },

         "rating": {
           "type": "object",
           "properties": {
             "type":{
               "enum": ["mpaa"]
             },
             "value": {
               "enum": ["G", "PG", "PG-13", "R", "NR"]
             },

             "explanation": {
               "type": "string",
               "minLength": 2
             },

             "text": {
               "type": "string",
               "minLength": 2
             }
           },
           "required": [ "type", "value", "explanation", "text" ],
           "additionalProperties": false
         },

         "releaseDates": {
           "type": "array",
           "items": {
             "type": "object",
               "properties": {
                 "type": {
                   "enum": ["theaters", "dvd"]
                 },
                 "date": {
                   "type": "number"
                 }
               },
               "required": [ "type", "date"],
               "additionalProperties": false
             },

           "minItems": 1,
           "uniqueItems": true
         },

         "awards": {
           "patternProperties": {
             "^[0-9]+$": {
               "type": "string",
               "minLength": 2
             }
           }
         },

         "images": {
           "type": "array",
           "items": {
             "type": "object",
             "properties": {

               "type": {
                 "enum": ["product", "screenshot"]
               },

               "url": {
                 "type": "string",
                 "format": "uri"
               }
             },

             "required": [ "type", "url"],
             "additionalProperties": false
           },

           "minItems": 1,
           "uniqueItems": true
         },


         "references": {
           "type": "array",
           "items": {
             "type": "object",
             "properties": {

               "type": {
                 "enum": ["upc", "netflix", "itunes", "googleplay", "asinproduct", "asinstream"]
               },

               "value": {
                 "type": "string",
                 "pattern": "^[0-9]+$"
               }
             },

             "required": [ "type", "value"],
             "additionalProperties": false
           },

           "minItems": 1,
           "uniqueItems": true
         },

         "id": { "type": "integer"}
       },

       "required": [ "cast", "directors", "genres", "length", "studios", "rating", "releaseDates", "awards", "images", "references", "id"],
       "additionalProperties": false
     },

    "slider": {
      "type": "object",
      "properties": {

        "status": {
          "type": "string",
          "enum": ["on", "off", "pause", "nfk"]
        },

        "not_for_kids": {
          "type": "integer",
          "minimum": 0,
          "maximum": 1
        },

        "text": {
          "type": "string",
          "minLength": 2
        },

        "off_age": {
          "type": "integer",
          "minimum": 2,
          "maximum": 17
        },

        "on_age": {
          "type": "integer",
          "minimum": 2,
          "maximum": 17
        },

        "target_age": {
          "type": "integer",
          "minimum": 2,
          "maximum": 17
        }
      },

      "required": [ "status", "text", "off_age", "on_age", "target_age"],
      "additionalProperties": false
    },

    "stars": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5
    },

    "talkingPoints": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 2

      },

      "minItems": 1,
      "maxItems": 5,
      "uniqueItems": true
    },

    "themes": {
      "type": "object",
      "patternProperties": {
        "^[0-9]+$": {
          "type": "string",
          "minLength": 2
        }
      },
      "minProperties": 5
    },

    "video": {
      "type": "object",
      "properties": {
        "files": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "type": {
                "enum": [
                  "flash",
                  "html5",
                  "mobile",
                  "html5_ff",
                  "mobile_ff"
                ]
              },
              "uri": {
                "type": "string",
                "format": "uri"
              },
              "thumbnail": {
                "type": "object",
                "properties": {
                  "small": {
                    "type": "string",
                    "minLength": 2
                  },
                  "large": {
                    "small": "string",
                    "minLength": 2
                  }
                },
                "required": [
                  "small",
                  "large"
                ],
                "additionalProperties": false
              }
            },
            "required": [
              "type",
              "uri",
              "thumbnail"
            ],
            "additionalProperties": false
          },
          "minItems": 1,
          "uniqueItems": true
        },
        "required": ["files"],
        "additionalProperties": false
      }
    },

    "stats": {
      "type": "object",
      "properties": {

        "csm_rating": {
          "type": "object",
          "properties": {
            "avg": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            },
            "count": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            }
          },

          "required": ["avg", "count"],
          "additionalProperties": false
        },

        "recommended_age": {
          "type": "object",
          "properties": {
            "avg": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            },
            "count": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            }
          },

          "required": ["avg", "count"],
          "additionalProperties": false
        },

        "stars": {
          "type": "object",
          "properties": {
            "avg": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[.0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            },
            "count": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            }
          },

          "required": ["avg", "count"],
          "additionalProperties": false
        },

        "user_review_count": {
          "type": "object",
          "properties": {
            "count": {
              "type": "object",
              "properties": {
                "adult": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "all": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "child": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                },
                "teen": {
                  "type": "string",
                  "pattern": "^[0-9]+$"
                }
              },
              "required": [
                "adult",
                "all",
                "child",
                "teen"
              ],
              "additionalProperties": false
            }
          },

          "required": ["avg", "count"],
          "additionalProperties": false
        }
      },

      "required": ["csm_rating", "recommended_age", "stars", "user_review_count"],
      "additionalProperties": false
    },

    "author": {
      "type": "string",
      "minLength": 2
    },

    "created": {
      "type": "integer"
    },

    "title": {
      "type": "string",
      "minLength": 2
    },

    "updated": {
      "type": "integer"
    },

    "id": {
      "type": "integer"
    },

    "author": {
      "url": "string",
      "format": "uri"
    },

    "channel": {
      "enum": ["movie", "game", "tv", "app", "music", "book", "website"]
    }
  }
}